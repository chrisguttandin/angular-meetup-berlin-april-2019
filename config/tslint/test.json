{
    "extends": "tslint-config-holy-grail",
    "rules": {
        "component-selector": [true, "element", "amb", "kebab-case"],
        "directive-selector": [true, "attribute", "amb", "camelCase"],
        "import-blacklist": false,
        "newline-per-chained-call": false,
        "no-implicit-dependencies": [true, "dev"],
        "no-lifecycle-call": false,
        "rxjs-suffix-subjects": false
    }
}
